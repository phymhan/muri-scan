leg = {'01 Inner Brow Raiser', ...
    '02 Outer Brow Raiser', ...
    '04 Brow Lowerer', ...
    '05 Upper Lid Raiser', ...
    '06 Cheek Raiser', ...
    '07 Lid Tightener', ...
    '09 Nose Wrinkler', ...
    '10 Upper Lip Raiser', ...
    '12 Lip Corner Puller', ...
    '14 Dimpler', ...
    '15 Lip Corner Depressor', ...
    '17 Chin Raiser', ...
    '20 Lip stretcher', ...
    '23 Lip Tightener', ...
    '25 Lips part', ...
    '26 Jaw Drop', ...
    '45 Blink', ...
    'Attention'};

A = readNPY('/media/ligong/Picasso/Active/NRI/A.npy');

% A = A >= 0.5;

t = 0:360/17:360-1;
x = sind(t);
y = cosd(t);
xy = [x(:), y(:)];
xy = [xy; 0, 0];
gplot(A, xy, '-o');
hold on
for i = 1:18
    if i < 9 || i == 18
        text(xy(i,1)+0.1, xy(i,2), leg{i}, 'fontsize', 12, 'interpreter', 'tex');
    else
        text(xy(i,1), xy(i,2), leg{i}, 'fontsize', 12, 'interpreter', 'tex', 'h);
    end
end
hold off
axis square
axis off
set(gcf, 'color', [1 1 1])
